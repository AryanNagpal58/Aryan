def standard_deviation(arr):
    sum = 0
    new_sum = 0
    for item in arr:
        sum += item

    length = len(arr)
    mean = sum / length

    for item in arr:
        new_sum += ((item - mean)**2)
    return (np.sqrt(new_sum)/ length)

print (standard_deviation(arr))


def min_index(arr):
    return (np.argmin(arr))

def max_index(arr):
    return (np.argmax(arr))

